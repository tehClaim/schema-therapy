{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "comma-dangle": ["error", "never"],
        "arrow-parens": ["error", "always"],
        "arrow-body-style": 0,
        "no-unused-vars": [
            "warn",
            {
                "vars": "local",
                "args": "none"
            }
        ],
        "react/jsx-filename-extension": 0,
        "react/jsx-one-expression-per-line": 0,
        "import/no-unresolved": 0
    }
}
